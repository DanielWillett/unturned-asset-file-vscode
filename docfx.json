{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "UnturnedAssetSpec",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "Docs/api"
    }
  ],
  "build": {
    "content": [
      {
        "src": "Docs",
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [ ]
      }
    ],
    "resource": [
      {
        "files": [ ]
      }
    ],
    "output": "Docs (Build)/_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "Unturned Asset Spec",
      "_appTitle": "Unturned Asset Spec",
      "_enableSearch": true,
      "pdf": false
    }
  }
}