{
    "$schema": "./Asset Spec Schema.json",
    "Type": "SDG.Unturned.ItemPlaceableAsset, Assembly-CSharp",
    "DisplayName": "Placeable Item",
    "Parent": "SDG.Unturned.ItemAsset, Assembly-CSharp",
    "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html",
    "Category": "ITEM",
    "VanillaIdLimit": 2000,
    "RequireId": true,
    "Properties":
    [
        {
            "Key": "Can_Zombies_Target",
            "Type": "Boolean",
            "DefaultValue": true,
            "Description": "Whether or not this item is eligible for zombies to detect and attack when stuck.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "CanZombiesTarget"
        },
        {
            "Key": "Items_Recovered_On_Salvage",
            "Type": "Int32",
            "DefaultValue": 1,
            "Minimum": 0,
            "Maximum": 100,
            "Description": "Number of items from @SalvageItem to recover when this item is salvaged at less than full health.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "MinItemsRecoveredOnSalvage",
            "ExclusiveWith": [ "Min_Items_Recovered_On_Salvage", "Max_Items_Recovered_On_Salvage" ]
        },
        {
            "Key": "Min_Items_Recovered_On_Salvage",
            "Type": "Int32",
            "Minimum": 0,
            "Maximum": "@Max_Items_Recovered_On_Salvage",
            "DefaultValue": "@Items_Recovered_On_Salvage",
            "Description": "Minimum number of items from @SalvageItem to recover when this item is salvaged at less than full health. A random value will be chosen between this and @Max_Items_Recovered_On_Salvage.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "MinItemsRecoveredOnSalvage"
        },
        {
            "Key": "Max_Items_Recovered_On_Salvage",
            "Type": "Int32",
            "Minimum": "@Min_Items_Recovered_On_Salvage",
            "Maximum": 100,
            "DefaultValue": "@Items_Recovered_On_Salvage",
            "Description": "Maximum number of items from @SalvageItem to recover when this item is salvaged at less than full health. A random value will be chosen between @Min_Items_Recovered_On_Salvage and this.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "MaxItemsRecoveredOnSalvage"
        },
        {
            "Key": "SalvageItem",
            "Type":
            {
                "Type": "AssetReference",
                "AssetTypes": [ "SDG.Unturned.ItemAsset, Assembly-CSharp", "SDG.Unturned.SpawnAsset, Assembly-CSharp" ],
                "SupportsThis": true
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "The item or spawn to give when salvaging this item at less than full health. Defaults to a random supply from the first blueprint on this asset that outputs only this item.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "SalvageItemRef"
        },
        {
            "Key": "Items_Recovered_On_Salvage_Full_Health",
            "Type": "Int32",
            "DefaultValue": 1,
            "Minimum": 0,
            "Maximum": 100,
            "Description": "Number of items from @SalvageItem_FullHealth to recover when this item is salvaged at full health.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "MinItemsRecoveredOnSalvageFullHealth",
            "ExclusiveWith": [ "Min_Items_Recovered_On_Salvage_Full_Health", "Max_Items_Recovered_On_Salvage_Full_Health" ],
            "Version": "3.26.1.0"
        },
        {
            "Key": "Min_Items_Recovered_On_Salvage_Full_Health",
            "Type": "Int32",
            "Minimum": 0,
            "Maximum": "@Max_Items_Recovered_On_Salvage_Full_Health",
            "DefaultValue": "@Items_Recovered_On_Salvage_Full_Health",
            "Description": "Minimum number of items from @SalvageItem_FullHealth to recover when this item is salvaged at full health. A random value will be chosen between this and @Max_Items_Recovered_On_Salvage_Full_Health.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "MinItemsRecoveredOnSalvageFullHealth",
            "Version": "3.26.1.0"
        },
        {
            "Key": "Max_Items_Recovered_On_Salvage_Full_Health",
            "Type": "Int32",
            "Minimum": "@Min_Items_Recovered_On_Salvage_Full_Health",
            "Maximum": 100,
            "DefaultValue": "@Items_Recovered_On_Salvage_Full_Health",
            "Description": "Maximum number of items from @SalvageItem_FullHealth to recover when this item is salvaged at full health. A random value will be chosen between @Min_Items_Recovered_On_Salvage_Full_Health and this.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "MaxItemsRecoveredOnSalvageFullHealth",
            "Version": "3.26.1.0"
        },
        {
            "Key": "SalvageItem_FullHealth",
            "Type":
            {
                "Type": "AssetReference",
                "AssetTypes": [ "SDG.Unturned.ItemAsset, Assembly-CSharp", "SDG.Unturned.SpawnAsset, Assembly-CSharp" ]
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "The item or spawn to give when salvaging this item at full health. Defaults to a random supply from the first blueprint on this asset that outputs only this item.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "SalvageItemFullHealthRef",
            "Version": "3.26.1.0"
        },
        {
            "Key": "Items_Dropped_On_Destroy",
            "Type": "Int32",
            "DefaultValue": 0,
            "Minimum": 0,
            "Maximum": 100,
            "Description": "Number of items from @Item_Dropped_On_Destroy to drop when this item is destroyed (not counting salvaging).",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "minItemsDroppedOnDestroy",
            "ExclusiveWith": [ "Min_Items_Dropped_On_Destroy", "Max_Items_Dropped_On_Destroy" ],
            "InclusiveWith": [ "Item_Dropped_On_Destroy" ]
        },
        {
            "Key": "Min_Items_Dropped_On_Destroy",
            "Type": "Int32",
            "Minimum": 0,
            "Maximum": "@Max_Items_Dropped_On_Destroy",
            "DefaultValue": "@Items_Dropped_On_Destroy",
            "Description": "Minimum number of items from @Item_Dropped_On_Destroy to drop when this item is destroyed (not counting salvaging). A random value will be chosen between this and @Max_Items_Dropped_On_Destroy.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "minItemsDroppedOnDestroy",
            "InclusiveWith": [ "Max_Items_Dropped_On_Destroy", "Item_Dropped_On_Destroy" ]
        },
        {
            "Key": "Max_Items_Dropped_On_Destroy",
            "Type": "Int32",
            "Minimum": "@Min_Items_Dropped_On_Destroy",
            "Maximum": 100,
            "DefaultValue": "@Items_Dropped_On_Destroy",
            "Description": "Maximum number of items from @Item_Dropped_On_Destroy to drop when this item is destroyed (not counting salvaging). A random value will be chosen between @Min_Items_Dropped_On_Destroy and this.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "maxItemsDroppedOnDestroy",
            "InclusiveWith": [ "Item_Dropped_On_Destroy" ]
        },
        {
            "Key": "Item_Dropped_On_Destroy",
            "Type":
            {
                "Type": "AssetReference",
                "AssetTypes": [ "SDG.Unturned.ItemAsset, Assembly-CSharp", "SDG.Unturned.SpawnAsset, Assembly-CSharp" ],
                "SupportsThis": true
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "The item or spawn to drop when this item is destroyed (not counting salvaging).",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "ItemDroppedOnDestroyRef"
        },
        {
            "Key": "PlaceableProvidesCraftingTags",
            "Type":
            {
                "Type": "List",
                "ElementType":
                {
                    "Type": "AssetReference",
                    "AssetType": "SDG.Unturned.TagAsset, Assembly-CSharp"
                }
            },
            "DefaultValue": [ ],
            "Description": "List of crafting tags to provide to nearby players when this item is placed.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties",
            "Variable": "PlaceableProvidedCraftingTags"
        },
        {
            "Key": "ExplosionEffect_CopyModelPosition",
            "Type": "Boolean",
            "Description": "Whether or not the placeable destroy effect should spawn exactly at the placeable's position instead of slightly below it.",
            "DefaultValue": false,
            "Variable": "ExplosionEffectFlags",
            "Version": "3.25.8.0",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties"
        },
        {
            "Key": "ExplosionEffect_CopyModelRotation",
            "Type": "Boolean",
            "Description": "Whether or not the placeable destroy effect should spawn with the placeable's rotation.",
            "DefaultValue": false,
            "Variable": "ExplosionEffectFlags",
            "Version": "3.25.8.0",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/placeable-asset.html#placeable-asset-properties"
        },
        {
            "Key": "Armor_FalloffMaxRange",
            "Type": "Float32",
            "DefaultValue": -1.0,
            "Minimum": "@Armor_FalloffRange",
            "Except": [ -1 ],
            "Description": "Ranged damage (guns) from greater than this distance finishes decreasing toward falloff multiplier. -1 indicates that falloff should be ignored.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/barricade-asset.html#barricade-asset-properties",
            "Variable": "ArmorFalloffMaxRange",
            "Version": "3.25.10.0"
        },
        {
            "Key": "Armor_FalloffRange",
            "Type": "Float32",
            "DefaultValue": "@Armor_FalloffMaxRange",
            "Minimum": 0.0,
            "Maximum": "@Armor_FalloffMaxRange",
            "Except": [ -1 ],
            "Description": "Ranged damage (guns) from greater than this distance begins decreasing toward falloff multiplier.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/barricade-asset.html#barricade-asset-properties",
            "Variable": "ArmorFalloffRange",
            "Version": "3.25.10.0"
        },
        {
            "Key": "Armor_FalloffMultiplier",
            "Type": "Float32",
            "DefaultValue": 1.0,
            "Minimum": 0.0,
            "Maximum": 1.0,
            "Except": [ -1 ],
            "Description": "Percentage of incoming damage to apply past @Armor_FalloffMaxRange.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/barricade-asset.html#barricade-asset-properties",
            "Variable": "ArmorFalloffMultiplier",
            "Version": "3.25.10.0"
        }
    ],
    "Types":
    [
        {
            "Type": "SDG.Unturned.EArmorTier, Assembly-CSharp",
            "DisplayName": "Armor Tier",
            "Docs": "https://unturned.wiki.gg/wiki/Gameplay_config#Barricades",
            "Values":
            [
                {
                    "Value": "LOW",
                    "Casing": "Low",
                    "Description": "Indicates that a barricade or structure takes more damage. 100% of damage is done by default, but this can be changed in the gameplay configuration file."
                },
                {
                    "Value": "HIGH",
                    "Casing": "High",
                    "Description": "Indicates that a barricade or structure takes less damage. 50% of damage is done by default, but this can be changed in the gameplay configuration file."
                }
            ]
        }
    ]
}