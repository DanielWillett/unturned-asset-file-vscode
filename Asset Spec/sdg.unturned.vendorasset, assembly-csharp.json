{
    "$schema": "./Asset Spec Schema.json",
    "Type": "SDG.Unturned.VendorAsset, Assembly-CSharp",
    "DisplayName": "Vendor (NPC Shop)",
    "Parent": "SDG.Unturned.Asset, Assembly-CSharp",
    "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html",
    "RequireId": false,
    "VanillaIdLimit": 2000,
    "Properties":
    [
        {
            "Key": "FaceOverride",
            "Type": "FaceIndex",
            "DefaultValue": null,
            "IncludedDefaultValue": 0,
            "Description": "Optional index of face image to use when this vendor is opened. Face is reset to character's default when unspecified or when a new message is opened.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#other-properties",
            "Variable": "faceOverride"
        },
        {
            "Key": "Buying",
            "Type":
            {
                "Type": "List",
                "Mode": "LegacyList",
                "CountType": "UInt8",
                "ElementType": "SDG.Unturned.VendorBuying, Assembly-CSharp"
            },
            "MaximumCount": 255,
            "DefaultValue": [ ],
            "Description": "Items being bought from players by the vendor shop.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#buying",
            "Variable": "buying"
        },
        {
            "Key": "Selling",
            "Type":
            {
                "Type": "List",
                "Mode": "LegacyList",
                "CountType": "UInt8",
                "ElementType": "SDG.Unturned.VendorSellingBase, Assembly-CSharp"
            },
            "MaximumCount": 255,
            "DefaultValue": [ ],
            "Description": "Items and vehicles being sold by the vendor shop to players.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#buying",
            "Variable": "selling"
        },
        {
            "Key": "Disable_Sorting",
            "Type": "Flag",
            "DefaultValue": false,
            "Description": "Disable sorting the buying and selling lists alphabetically.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#other-properties",
            "Variable": "enableSorting"
        },
        {
            "Key": "Currency",
            "Type":
            {
                "Type": "AssetReferenceString",
                "AssetType": "SDG.Unturned.ItemCurrencyAsset, Assembly-CSharp"
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "GUID of the currency asset to use as currency instead of experience points.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#other-properties",
            "Variable": "currency"
        }
    ],
    "Types":
    [
        {
            "Type": "SDG.Unturned.VendorBuying, Assembly-CSharp",
            "DisplayName": "Vendor Procurement",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#buying",
            "Properties":
            [
                {
                    "Key": "ID",
                    "Type":
                    {
                        "Type": "LegacyAssetReferenceString",
                        "AssetType": "SDG.Unturned.ItemAsset, Assembly-CSharp"
                    },
                    "DefaultValue": "00000000-0000-0000-0000-000000000000",
                    "Description": "GUID or ID of the item to buy from the player.",
                    "Variable": "TargetAssetGuid",
                    "Required": true
                },
                {
                    "Key": "Cost",
                    "Type": "UInt32",
                    "DefaultValue": 0,
                    "Description": "Amount of currency to pay the player.",
                    "Variable": "cost"
                },
                {
                    "Key": "Conditions",
                    "Type":
                    {
                        "Type": "List",
                        "ElementType": "SDG.Unturned.INPCCondition, Assembly-CSharp",
                        "Mode": "LegacyList, ModernList"
                    },
                    "DefaultValue": [ ],
                    "Description": "Conditions that must be met for players to be able to sell item #(Self.TemplateGroups[0]).",
                    "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/conditions.html",
                    "Variable": "conditionsList"
                },
                {
                    "Key": "Rewards",
                    "Type":
                    {
                        "Type": "List",
                        "ElementType": "SDG.Unturned.INPCReward, Assembly-CSharp",
                        "Mode": "LegacyList, ModernList"
                    },
                    "DefaultValue": [ ],
                    "Description": "Rewards applied after the player sells item #(Self.TemplateGroups[0]).",
                    "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/rewards.html",
                    "Variable": "rewardsList"
                }
            ],
            "Localization":
            [
                {
                    "Key": "Description",
                    "Type":
                    {
                        "Type": "String",
                        "SupportsRichText": true
                    },
                    "DefaultValue": null,
                    "Description": "If set, overrides item description shown in the vendor menu. I.e., how the vendor would describe the item rather than how the player would.",
                    "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#localization",
                    "Variable": "descriptionOverride"
                }
            ]
        },
        {
            "Type": "SDG.Unturned.VendorSellingBase, Assembly-CSharp",
            "DisplayName": "Vendor Listing",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#selling",
            "Properties":
            [
                {
                    "Key": "Type",
                    "Type": "DanielWillett.UnturnedDataFileLspServer.Data.Types.VendorSellingBaseType, UnturnedAssetSpec",
                    "DefaultValue": "Item",
                    "Description": "Whether this vendor listing is for an item or vehicle."
                },
                {
                    "Key": "ID",
                    "Type":
                    [
                        {
                            "Case":
                            {
                                "Variable": "Type",
                                "Operation": "eq",
                                "Comparand": "Vehicle"
                            },
                            "Value":
                            {
                                "Type": "LegacyAssetReferenceString",
                                "AssetType": "SDG.Unturned.VehicleAsset, Assembly-CSharp"
                            }
                        },
                        {
                            "Value":
                            {
                                "Type": "LegacyAssetReferenceString",
                                "AssetType": "SDG.Unturned.ItemAsset, Assembly-CSharp"
                            }
                        }
                    ],
                    "Description":
                    [
                        {
                            "Case":
                            {
                                "Variable": "Type",
                                "Operation": "eq",
                                "Comparand": "Vehicle"
                            },
                            "Value": "GUID or ID of the vehicle to buy from the player."
                        },
                        {
                            "Value": "GUID or ID of the item to buy from the player."
                        }
                    ],
                    "Variable": "TargetAssetGuid",
                    "Required": true
                },
                {
                    "Key": "Cost",
                    "Type": "UInt32",
                    "DefaultValue": 0,
                    "Description": "Amount of currency to pay the vendor.",
                    "Variable": "cost"
                },
                {
                    "Key": "Conditions",
                    "Type":
                    {
                        "Type": "List",
                        "ElementType": "SDG.Unturned.INPCCondition, Assembly-CSharp",
                        "Mode": "LegacyList, ModernList"
                    },
                    "DefaultValue": [ ],
                    "Description": "Conditions that must be met for players to be able to buy listing #(Self.TemplateGroups[0]).",
                    "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/conditions.html",
                    "Variable": "conditionsList"
                },
                {
                    "Key": "Rewards",
                    "Type":
                    {
                        "Type": "List",
                        "ElementType": "SDG.Unturned.INPCReward, Assembly-CSharp",
                        "Mode": "LegacyList, ModernList"
                    },
                    "DefaultValue": [ ],
                    "Description": "Rewards applied after the player buys listing #(Self.TemplateGroups[0]).",
                    "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/rewards.html",
                    "Variable": "rewardsList"
                },
                {
                    "Key": "Sight",
                    "Type":
                    {
                        "Type": "DefaultableLegacyAssetReference",
                        "AssetType": "SDG.Unturned.ItemSightAsset, Assembly-CSharp"
                    },
                    "DefaultValue": -1,
                    "Description": "Override for the sight attachment that should be attached to the item sold.",
                    "Variable": "sight",
                    "InclusiveWith":
                    [
                        {
                            "Key": "Type",
                            "Value": "Item"
                        },
                        {
                            "Key": "ID",
                            "Condition":
                            {
                                "Variable": "ID",
                                "Operation": "is-type",
                                "Comparand": "SDG.Unturned.ItemGunAsset, Assembly-CSharp"
                            }
                        }
                    ]
                },
                {
                    "Key": "Tactical",
                    "Type":
                    {
                        "Type": "DefaultableLegacyAssetReference",
                        "AssetType": "SDG.Unturned.ItemTacticalAsset, Assembly-CSharp"
                    },
                    "DefaultValue": -1,
                    "Description": "Override for the tactical attachment that should be attached to the item sold.",
                    "Variable": "tactical",
                    "InclusiveWith":
                    [
                        {
                            "Key": "Type",
                            "Value": "Item"
                        },
                        {
                            "Key": "ID",
                            "Condition":
                            {
                                "Variable": "ID",
                                "Operation": "is-type",
                                "Comparand": "SDG.Unturned.ItemGunAsset, Assembly-CSharp"
                            }
                        }
                    ]
                },
                {
                    "Key": "Grip",
                    "Type":
                    {
                        "Type": "DefaultableLegacyAssetReference",
                        "AssetType": "SDG.Unturned.ItemGripAsset, Assembly-CSharp"
                    },
                    "DefaultValue": -1,
                    "Description": "Override for the grip attachment that should be attached to the item sold.",
                    "Variable": "grip",
                    "InclusiveWith":
                    [
                        {
                            "Key": "Type",
                            "Value": "Item"
                        },
                        {
                            "Key": "ID",
                            "Condition":
                            {
                                "Variable": "ID",
                                "Operation": "is-type",
                                "Comparand": "SDG.Unturned.ItemGunAsset, Assembly-CSharp"
                            }
                        }
                    ]
                },
                {
                    "Key": "Barrel",
                    "Type":
                    {
                        "Type": "DefaultableLegacyAssetReference",
                        "AssetType": "SDG.Unturned.ItemBarrelAsset, Assembly-CSharp"
                    },
                    "DefaultValue": -1,
                    "Description": "Override for the barrel attachment that should be attached to the item sold.",
                    "Variable": "barrel",
                    "InclusiveWith":
                    [
                        {
                            "Key": "Type",
                            "Value": "Item"
                        },
                        {
                            "Key": "ID",
                            "Condition":
                            {
                                "Variable": "ID",
                                "Operation": "is-type",
                                "Comparand": "SDG.Unturned.ItemGunAsset, Assembly-CSharp"
                            }
                        }
                    ]
                },
                {
                    "Key": "Magazine",
                    "Type":
                    {
                        "Type": "DefaultableLegacyAssetReference",
                        "AssetType": "SDG.Unturned.ItemMagazineAsset, Assembly-CSharp"
                    },
                    "DefaultValue": -1,
                    "Description": "Override for the magazine attachment that should be attached to the item sold.",
                    "Variable": "magazine",
                    "InclusiveWith":
                    [
                        {
                            "Key": "Type",
                            "Value": "Item"
                        },
                        {
                            "Key": "ID",
                            "Condition":
                            {
                                "Variable": "ID",
                                "Operation": "is-type",
                                "Comparand": "SDG.Unturned.ItemGunAsset, Assembly-CSharp"
                            }
                        }
                    ]
                },
                {
                    "Key": "Ammo",
                    "Type": "Int32",
                    "Minimum": 0,
                    "Maximum": 255,
                    "Except": [ -1 ],
                    "DefaultValue": -1,
                    "Description": "Override for the amount of ammuntion that should be loaded in the item sold.",
                    "Variable": "ammo",
                    "InclusiveWith":
                    [
                        {
                            "Key": "Type",
                            "Value": "Item"
                        },
                        {
                            "Key": "ID",
                            "Condition":
                            {
                                "Variable": "ID",
                                "Operation": "is-type",
                                "Comparand": "SDG.Unturned.ItemGunAsset, Assembly-CSharp"
                            }
                        }
                    ]
                },
                {
                    "Key": "Spawnpoint",
                    "Type": "SpawnpointId",
                    "DefaultValue": null,
                    "Required":
                    {
                        "Variable": "Type",
                        "Operation": "eq",
                        "Comparand": "Vehicle"
                    },
                    "Description": "Location to spawn the purchased vehicle, using the ID of a spawnpoint node as set in the level editor.",
                    "Variable": "spawnpoint",
                    "InclusiveWith":
                    [
                        {
                            "Key": "Type",
                            "Value": "Vehicle"
                        }
                    ]
                },
                {
                    "Key": "PaintColor",
                    "Type": "Color32",
                    "DefaultValue": null,
                    "Required":
                    {
                        "Variable": "Type",
                        "Operation": "eq",
                        "Comparand": "Vehicle"
                    },
                    "Description": "Location to spawn the purchased vehicle, using the ID of a spawnpoint node as set in the level editor.",
                    "Variable": "paintColor",
                    "FileCrossRef": "ID",
                    "InclusiveWith":
                    [
                        {
                            "Key": "Type",
                            "Value": "Vehicle"
                        }
                    ],
                    "ExclusiveWith":
                    [
                        {
                            "Key": "$cr$::SDG.Unturned.VehicleAsset, Assembly-CSharp::PaintableSections",
                            "Value": null
                        }
                    ]
                }
            ],
            "Localization":
            [
                {
                    "Key": "Description",
                    "Type":
                    {
                        "Type": "String",
                        "SupportsRichText": true
                    },
                    "DefaultValue": null,
                    "Description": "If set, overrides item description shown in the vendor menu. I.e., how the vendor would describe the item rather than how the player would.",
                    "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#localization",
                    "Variable": "descriptionOverride"
                }
            ]
        },
        {
            "Type": "DanielWillett.UnturnedDataFileLspServer.Data.Types.VendorSellingBaseType, UnturnedAssetSpec",
            "DisplayName": "Vendor Listing Type",
            "Values":
            [
                {
                    "Value": "Item",
                    "Description": "This vendor listing is selling an item.",
                    "CorrespondingType": "SDG.Unturned.VendorSellingItem, Assembly-CSharp"
                },
                {
                    "Value": "Vehicle",
                    "Description": "This vendor listing is selling a vehicle.",
                    "CorrespondingType": "SDG.Unturned.VendorSellingVehicle, Assembly-CSharp"
                }
            ]
        }
    ],
    "Localization":
    [
        {
            "Key": "Name",
            "Type":
            {
                "Type": "String",
                "SupportsRichText": true
            },
            "DefaultValue": "Name",
            "Description": "Localized name of the vendor shop.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#localization",
            "Variable": "vendorName"
        },
        {
            "Key": "Description",
            "Type":
            {
                "Type": "String",
                "SupportsRichText": true,
                "SupportsNewLines": true
            },
            "DefaultValue": "Description",
            "Description": "Localized description of the vendor shop.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/npcs/vendor-asset.html#localization",
            "Variable": "vendorDescription"
        }
    ]
}