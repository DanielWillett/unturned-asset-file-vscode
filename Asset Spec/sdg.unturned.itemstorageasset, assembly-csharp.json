{
    "$schema": "./Asset Spec Schema.json",
    "Type": "SDG.Unturned.ItemStorageAsset, Assembly-CSharp",
    "DisplayName": "Storage Barricade Item",
    "Parent": "SDG.Unturned.ItemBarricadeAsset, Assembly-CSharp",
    "Docs": "https://docs.smartlydressedgames.com/en/stable/items/storage-asset.html",
    "Category": "ITEM",
    "VanillaIdLimit": 2000,
    "RequireId": true,
    "Properties":
    [
        {
            "Key": "Storage_X",
            "Type": "UInt8",
            "DefaultValue": 1,
            "Minimum": 1,
            "Description": "Number of columns (horizontal storage space).",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/storage-asset.html#storage-asset-properties",
            "Variable": "storage_x"
        },
        {
            "Key": "Storage_Y",
            "Type": "UInt8",
            "DefaultValue": 1,
            "Minimum": 1,
            "Description": "Number of rows (vertical storage space).",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/storage-asset.html#storage-asset-properties",
            "Variable": "storage_y"
        },
        {
            "Key": "Display",
            "Type": "Flag",
            "DefaultValue": false,
            "Description": "The first item in this storage will be visibly displayed.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/storage-asset.html#storage-asset-properties",
            "Variable": "isDisplay"
        },
        {
            "Key": "Should_Close_When_Outside_Range",
            "Type": "Boolean",
            "DefaultValue": false,
            "Description": "Whether or not the storage should automatically close when the player is outside of the interaction range.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/storage-asset.html#storage-asset-properties",
            "Variable": "shouldCloseWhenOutsideRange"
        },
        {
            "Key": "Can_Players_Open",
            "Type": "Boolean",
            "DefaultValue": true,
            "Description": "Whether or not players can interact with the barricade to open its storage.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/storage-asset.html#storage-asset-properties",
            "Variable": "CanPlayersOpen",
            "Version": "3.26.1.0"
        },
        {
            "Key": "Delete_Contained_Items_On_Destroy",
            "Type": "Boolean",
            "DefaultValue": true,
            "Description": "Whether or not any stored items are despawned rather than dropped when destroyed.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/storage-asset.html#storage-asset-properties",
            "Variable": "ShouldDeleteContainedItemsOnDestroy",
            "Version": "3.26.1.0"
        },
        {
            "Key": "Default_Contained_Items",
            "Type":
            {
                "Type": "List",
                "ElementType": "SDG.Unturned.LevelAsset+DefaultLoadoutItem, Assembly-CSharp"
            },
            "DefaultValue": null,
            "Description": "Default items to add to the storage when it's placed..",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/storage-asset.html#storage-asset-properties",
            "Variable": "DefaultContainedItems",
            "Version": "3.25.10.0"
        }
    ]
}