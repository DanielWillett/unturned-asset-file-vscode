{
    "$schema": "./Asset Spec Schema.json",
    "Type": "SDG.Unturned.ItemMagazineAsset, Assembly-CSharp",
    "DisplayName": "Magazine Attachment Item",
    "Parent": "SDG.Unturned.ItemCaliberAsset, Assembly-CSharp",
    "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html",
    "Category": "ITEM",
    "VanillaIdLimit": 2000,
    "RequireId": true,
    "Properties":
    [
        {
            "Key": "Pellets",
            "Type": "UInt8",
            "Minimum": 1,
            "DefaultValue": 1,
            "Description": "Number of pellets from each bullet/shell.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#pellets-uint8-1",
            "Variable": "pellets",
            "Priority": 10
        },
        {
            "Key": "Stuck",
            "Type": "UInt8",
            "DefaultValue": 0,
            "Description": "The amount of quality that should be lost after the projectile hits something.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#stuck-uint8-0",
            "Variable": "stuck",
            "Priority": 7
        },
        {
            "Key": "Projectile_Damage_Multiplier",
            "Type": "Float32",
            "DefaultValue": 1.0,
            "Description": "Multiplier on the damage dealt by explosive projectiles fired from weapons. Doesn't have any effect when used in a ballistic gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#projectile-damage-multiplier-float32-1",
            "Variable": "projectileDamageMultiplier",
            "Priority": 6
        },
        {
            "Key": "Projectile_Blast_Radius_Multiplier",
            "Type": "Float32",
            "DefaultValue": 1.0,
            "Description": "Multiplier on the blast radius of explosive projectiles fired from weapons. Doesn't have any effect when used in a ballistic gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#projectile-blast-radius-multiplier-float32-1",
            "Variable": "projectileBlastRadiusMultiplier",
            "Priority": 6
        },
        {
            "Key": "Projectile_Launch_Force_Multiplier",
            "Type": "Float32",
            "DefaultValue": 1.0,
            "Description": "Multiplier on the player launch force of explosive projectiles fired from weapons. Doesn't have any effect when used in a ballistic gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#projectile-launch-force-multiplier-float32-1",
            "Variable": "projectileLaunchForceMultiplier",
            "Priority": 6
        },
        {
            "Key": "Range",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Description": "Range (in meters) of the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#pellets-uint8-1",
            "Variable": "range",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Player_Damage",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Description": "Damage dealt to players from the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#player-damage-float32-0",
            "Variable": "playerDamage",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Zombie_Damage",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Description": "Damage dealt to zombies from the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#zombie-damage-float32-0",
            "Variable": "zombieDamage",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Animal_Damage",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Description": "Damage dealt to animals from the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#animal-damage-float32-0",
            "Variable": "animalDamage",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Barricade_Damage",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Description": "Damage dealt to barricades from the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#barricade-damage-float32-0",
            "Variable": "barricadeDamage",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Structure_Damage",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Description": "Damage dealt to structures from the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#structure-damage-float32-0",
            "Variable": "structureDamage",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Vehicle_Damage",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Description": "Damage dealt to vehicles from the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#vehicle-damage-float32-0",
            "Variable": "vehicleDamage",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Resource_Damage",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Description": "Damage dealt to resources from the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#resource-damage-float32-0",
            "Variable": "resourceDamage",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Explosion_Launch_Speed",
            "Type": "Float32",
            "DefaultValue": "=MUL(@Player_Damage 0.1)",
            "Description": "Player launch speed of the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#explosion-launch-speed-float32",
            "Variable": "explosionLaunchSpeed",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Explosion_Plays_Impact_Effects",
            "Type": "Boolean",
            "DefaultValue": true,
            "Description": "Whether or not per-surface effects like blood splatter are created. Defaults to true, but can be disabled particularly if a performance issue (e.g., an explosive many-pellet shotgun shell).",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#explosion-plays-impact-effects-bool-true",
            "Variable": "ExplosionPlaysImpactEffects",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ],
            "Version": "3.25.10.0"
        },
        {
            "Key": "Explosion_Penetrate_Buildables",
            "Type": "Boolean",
            "DefaultValue": false,
            "Description": "Whether or not explosion damage passes through buildable items.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#explosion-plays-impact-effects-bool-true",
            "Variable": "ExplosionPenetratesBuildables",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ],
            "Version": "3.25.10.0"
        },
        {
            "Key": "Explosion",
            "Type":
            {
                "Type": "LegacyAssetReferenceString",
                "AssetType": "SDG.Unturned.EffectAsset, Assembly-CSharp"
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "GUID or legacy ID of the explosion set off by when a bullet lands if the @Explosive flag is present, or when a projectile explodes, overriding the explosion effect set in the gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#explosion-doc-data-guid-or-uint16-0",
            "Variable": "explosionEffectGuid",
            "Priority": 4
        },
        {
            "Key": "Object_Damage",
            "Type": "Float32",
            "IncludedDefaultValue": 0.0,
            "DefaultValue": "@Resource_Damage",
            "Description": "Damage dealt to objects from the explosion set off when a bullet lands if the @Explosive flag is present. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#object-damage-float32",
            "Variable": "objectDamage",
            "Priority": 3,
            "InclusiveWith": [ "Explosive" ]
        },
        {
            "Key": "Tracer",
            "Type":
            {
                "Type": "LegacyAssetReferenceString",
                "AssetType": "SDG.Unturned.EffectAsset, Assembly-CSharp"
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "GUID or legacy ID of the effect used for the bullet tracer. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#tracer-doc-data-guid-or-uint16-0",
            "Variable": "tracerEffectGuid",
            "Priority": 8
        },
        {
            "Key": "Impact",
            "Type":
            {
                "Type": "LegacyAssetReferenceString",
                "AssetType": "SDG.Unturned.EffectAsset, Assembly-CSharp"
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "GUID or legacy ID of the effect used for the bullet impact. Doesn't have any effect when used in a projectile gun. The default bullet impact effect is fetched from the impacted physics material.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#impact-doc-data-guid-or-uint16-0",
            "Variable": "ImpactEffectGuid",
            "Priority": 8
        },
        {
            "Key": "Speed",
            "Type": "Float32",
            "Minimum": 0.01,
            "DefaultValue": 1.0,
            "Description": "Reload and hammer speed multiplier.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#speed-float32-1",
            "Variable": "speed",
            "Priority": 7
        },
        {
            "Key": "Explosive",
            "Type": "Flag",
            "DefaultValue": false,
            "Description": "This magazine's bullets will create an explosion when they land. Doesn't have any effect when used in a projectile gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#explosive-flag",
            "Variable": "isExplosive",
            "Priority": 5
        },
        {
            "Key": "Spawn_Explosion_On_Dedicated_Server",
            "Type": "Flag",
            "DefaultValue": false,
            "Description": "This magazine's @Explosion effect will be spawned on dedicated servers.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#spawn-explosion-on-dedicated-server-flag",
            "Variable": "spawnExplosionOnDedicatedServer",
            "Priority": 2
        },
        {
            "Key": "Should_Fill_After_Detach",
            "Type": "Boolean",
            "DefaultValue": false,
            "Description": "Whether or not this magazine should be refilled when removed from a gun.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#should-fill-after-detach-bool-false",
            "Variable": "shouldFillAfterDetach",
            "Priority": 7
        }
    ],
    "BundleAssets":
    [
        {
            "Key": "Magazine",
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Description": "The model that appears on the gun when the item is attached.",
            "Variable": "magazine",
            "Required": true
        },
        {
            "Key": "Projectile",
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Description": "Overrides Projectile prefab. Note that the gun this magazine is attached to must launch projectiles.",
            "Variable": "ProjectilePrefabOverride",
            "Required": true,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/magazine-asset.html#unity-setup",
            "Version": "3.25.10.0"
        }
    ]
}