            "Required": true,
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "AnimationComponentFindArgument": "Character",
            "Animations":
            [
                {
                    "Name": "Idle",
                    "Required": true
                },
                {
                    "Name": "Walk",
                    "Required": true
                },
                {
                    "Name": "Run",
                    "Required": true
                },
                {
                    "Name": "Attack_(\\d+)",
                    "AssetNameIsRegex": true,
                    "AssetNameGroups":
                    [
                        {
                            "Name": "animal-anim-attack",
                            "RegexGroup": 1
                        }
                    ]
                },
                {
                    "Name": "Eat_(\\d+)",
                    "SingleNameOverride": "Eat",
                    "AssetNameIsRegex": true,
                    "AssetNameGroups":
                    [
                        {
                            "Name": "animal-anim-eat",
                            "RegexGroup": 1
                        }
                    ]
                },
                {
                    "Name": "Glance_(\\d+)",
                    "AssetNameIsRegex": true,
                    "AssetNameGroups":
                    [
                        {
                            "Name": "animal-anim-glance",
                            "RegexGroup": 1
                        }
                    ]
                },
                {
                    "Name": "Startle_(\\d+)",
                    "SingleNameOverride": "Startle",
                    "AssetNameIsRegex": true,
                    "AssetNameGroups":
                    [
                        {
                            "Name": "animal-anim-startle",
                            "RegexGroup": 1
                        }
                    ]
                }
            ],
            "Variable": "client"
        },
        {
            "AssetName": "Animal_Server",
            "Description": "The model loaded on local clients hosting a server or singleplayer.",
            "Required": true,
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "AnimationComponentFindArgument": "Character",
            "Animations":
            [
                {
                    "Name": "Idle",
                    "Required": true
                },
                {
                    "Name": "Walk",
                    "Required": true
                },
                {
                    "Name": "Run",
                    "Required": true
                },
                {
                    "Name": "Attack_(\\d+)",
                    "AssetNameIsRegex": true,
                    "AssetNameGroups":
                    [
                        {
                            "Name": "animal-anim-attack",
                            "RegexGroup": 1
                        }
                    ]
                },
                {
                    "Name": "Eat_(\\d+)",
                    "SingleNameOverride": "Eat",
                    "AssetNameIsRegex": true,
                    "AssetNameGroups":
                    [
                        {
                            "Name": "animal-anim-eat",
                            "RegexGroup": 1
                        }
                    ]
                },
                {
                    "Name": "Glance_(\\d+)",
                    "AssetNameIsRegex": true,
                    "AssetNameGroups":
                    [
                        {
                            "Name": "animal-anim-glance",
                            "RegexGroup": 1
                        }
                    ]
                },
                {
                    "Name": "Startle_(\\d+)",
                    "SingleNameOverride": "Startle",
                    "AssetNameIsRegex": true,
                    "AssetNameGroups":
                    [
                        {
                            "Name": "animal-anim-startle",
                            "RegexGroup": 1
                        }
                    ]
                }
            ],
            "Variable": "server"
        },
        {
            "AssetName": "Animal_Dedicated",
            "Description": "The model loaded on dedicated servers.",
            "Required": true,
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Variable": "dedicated"
        },
        {
            "AssetName": "Ragdoll",
            "Description": "The model loaded on clients when the animal is killed.",
            "Required": true,
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Variable": "ragdoll"
        },
        {
            "AssetName": "Roar_(\\d+)",
            "AssetNameIsRegex": true,
            "AssetNameGroups":
            [
                {
                    "Name": "animal-roars",
