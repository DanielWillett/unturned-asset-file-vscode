            "Values":
            [
                {
                    "Value": "STAND",
                    "Casing": "Stand",
                    "Description": "Standing straight up with the NPC's arms at their side or on their hips. The NPC will occasionally fidget or look around."
                },
                {
                    "Value": "SIT",
                    "Casing": "Sit",
                    "Description": "Sitting as if in a chair with the NPC's hands on their knees."
                },
                {
                    "Value": "ASLEEP",
                    "Casing": "Asleep",
                    "Description": "Standing straight up with the NPC's arms at their side with minimal movement. Can be rotated to be in a laying position."
                },
                {
                    "Value": "PASSIVE",
                    "Casing": "Passive",
                    "Description": "Standing straight up with the NPC's arms at their side or on their hips. The NPC will occasionally fidget or look around. If a weapon is equipped it will always be on safety."
                },
                {
                    "Value": "CROUCH",
                    "Casing": "Crouch",
                    "Description": "Crouching down with the NPC's arms at their side with minimal movement."
                },
                {
                    "Value": "PRONE",
                    "Casing": "Prone",
                    "Description": "Laying prone on the ground with the NPC's arms in front of them."
                },
                {
                    "Value": "UNDER_ARREST",
                    "Casing": "Under_Arrest",
                    "Description": "Standing straight up with the NPC's arms behind their back as if they were handcuffed."
                },
                {
                    "Value": "REST",
                    "Casing": "Rest",
                    "Description": "Sitting on the ground with the NPC's hands in their lap."
                },
                {
                    "Value": "SURRENDER",
                    "Casing": "Surrender",
                    "Description": "Standing straight up with the NPC's hands in the air."
                }
            ]
        }
