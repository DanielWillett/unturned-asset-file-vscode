{
    "$schema": "./Asset Spec Schema.json",
    "Type": "SDG.Unturned.ResourceAsset, Assembly-CSharp",
    "DisplayName": "Resource",
    "Parent": "SDG.Unturned.Asset, Assembly-CSharp",
    "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html",
    "Category": "RESOURCE",
    "RequireId": true,
    "VanillaIdLimit": 50,
    "Properties":
    [
        {
            "Key": "Bypass_ID_Limit",
            "Type": "Flag",
            "DefaultValue": false,
            "Description": "Allows IDs to be less than 50 if this isn't the vanilla bundle.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties"
        },
        {
            "Key": "Has_Clip_Prefab",
            "Type": "Boolean",
            "DefaultValue": true,
            "Description": "Whether or not this resource defines separate prefabs (@($bndl$::Resource_Clip), @($bndl$::Stump_Clip)) to be loaded on the server. This is not required and should be set to false in most cases."
        },
        {
            "Key": "Auto_Skybox",
            "Type": "Boolean",
            "DefaultValue": true,
            "Description": "Automatically render a card texture on startup and apply the texture to the @($bndl$::Skybox) model ('Model_0').",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "InclusiveWith": [ "$bndl$::Skybox" ]
        },
        {
            "Key": "Health",
            "Type": "UInt16",
            "MinimumExclusive": 0,
            "DefaultValue": 0,
            "Required": true,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "The total health of this resource.",
            "Variable": "health"
        },
        {
            "Key": "RandomAngleDeviation_Min",
            "Type": "Float32",
            "Minimum": -180.0,
            "Maximum": "@RandomAngleDeviation_Max",
            "DefaultValue": -5.0,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Minimum angle in degrees away from the up direction, where 0 is upright.",
            "Variable": "MinRandomAngleDeviation"
        },
        {
            "Key": "RandomAngleDeviation_Max",
            "Type": "Float32",
            "Minimum": "@RandomAngleDeviation_Min",
            "Maximum": 180.0,
            "DefaultValue": 5.0,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Maximum angle in degrees away from the up direction, where 0 is upright.",
            "Variable": "MaxRandomAngleDeviation"
        },
        {
            "Key": "Scale",
            "Type": "Float32",
            "Deprecated": true,
            "Minimum": 1.0,
            "DefaultValue":
            [
                {
                    "Case":
                    {
                        "Variable": "#Scale.Included{RequireValue=true}",
                        "Operation": "eq",
                        "Comparand": true
                    },
                    "Value": 0.0
                },
                {
                    "Value": "=MUL(=SUB(@RandomUniformScale_Max @RandomUniformScale_Min) 0.5)"
                }
            ],
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "The tree's in-game scale is a random number between 1.1 and the result of 1.1 + (Scale * 2).",
            "Variable": "scale",
            "ExclusiveWith": [ "RandomUniformScale_Min", "RandomUniformScale_Max" ]
        },
        {
            "Key": "RandomUniformScale_Min",
            "Type": "Float32",
            "Maximum": "@RandomUniformScale_Max",
            "DefaultValue": 1.1,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Minimum scale. The same randomized value is used for all axes (uniform).",
            "Variable": "MinRandomUniformScale"
        },
        {
            "Key": "RandomUniformScale_Max",
            "Type": "Float32",
            "Minimum": "@RandomUniformScale_Min",
            "DefaultValue":
            [
                {
                    "Case":
                    {
                        "Variable": "#Scale.Included{RequireValue=true}",
                        "Operation": "eq",
                        "Comparand": true
                    },
                    "Value": "=ADD(1.1 =MUL(@Scale 2.0))"
                },
                {
                    "Value": 1.1
                }
            ],
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Maximum scale. The same randomized value is used for all axes (uniform).",
            "Variable": "MaxRandomUniformScale"
        },
        {
            "Key": "Vertical_Offset",
            "Type": "Float32",
            "DefaultValue": -0.75,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "A vertical offset above or below wherever this resource is placed, in meters.",
            "Variable": "verticalOffset"
        },
        {
            "Key": "Debris_Vertical_Offset",
            "Type": "Float32",
            "DefaultValue": 1.0,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Distance along tree's local up axis to offset debris spawn position.",
            "Variable": "DebrisVerticalOffset"
        },
        {
            "Key": "Explosion",
            "Type":
            {
                "Type": "LegacyAssetReferenceString",
                "AssetType": "SDG.Unturned.EffectAsset, Assembly-CSharp"
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Effect to play when this resource is destroyed.",
            "Variable": "explosionGuid"
        },
        {
            "Key": "Log",
            "Type":
            {
                "Type": "LegacyAssetReference",
                "AssetType": "SDG.Unturned.ItemAsset, Assembly-CSharp"
            },
            "DefaultValue": 0,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "ID of an item that should be dropped when the resource is destroyed. Before multipliers, this item is dropped in bunches of 3 to 7. This is also the item that is given to the player when this resource is foraged.",
            "Variable": "log",
            "ExclusiveWith": [ "Reward_ID" ]
        },
        {
            "Key": "Stick",
            "Type":
            {
                "Type": "LegacyAssetReference",
                "AssetType": "SDG.Unturned.ItemAsset, Assembly-CSharp"
            },
            "DefaultValue": 0,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "ID of an item that should be dropped when the resource is destroyed. Before multipliers, this item is dropped in bunches of 2 to 5.",
            "Variable": "stick",
            "ExclusiveWith":
            [
                "Reward_ID",
                {
                    "Key": "Forage",
                    "Value": true
                }
            ]
        },
        {
            "Key": "Reward_ID",
            "Type":
            {
                "Type": "LegacyAssetReference",
                "AssetType": "SDG.Unturned.SpawnAsset, Assembly-CSharp"
            },
            "DefaultValue": 0,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "ID of an item spawn table to pull items from that should be dropped when this resource is destroyed or foraged.",
            "Variable": "rewardID",
            "ExclusiveWith": [ "Log", "Stick" ]
        },
        {
            "Key": "Reward_XP",
            "Type": "UInt32",
            "DefaultValue": 0,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Amount of experience to reward when the resource is destroyed.",
            "Variable": "rewardXP",
            "ExclusiveWith":
            [
                {
                    "Key": "Forage",
                    "Value": true
                }
            ]
        },
        {
            "Key": "Reward_Min",
            "Type": "UInt8",
            "DefaultValue": 6,
            "IncludedDefaultValue": 0,
            "Maximum": "@Reward_Max",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Minimum amount of item drops to reward.",
            "Variable": "rewardMin",
            "InclusiveWith":
            [
                {
                    "Key": "Reward_ID",
                    "Condition":
                    {
                        "Variable": "Reward_ID",
                        "Operation": "gt",
                        "Comparand": 0
                    }
                }
            ],
            "ExclusiveWith":
            [
                {
                    "Key": "Forage",
                    "Value": true
                }
            ]
        },
        {
            "Key": "Reward_Max",
            "Type": "UInt8",
            "DefaultValue": 9,
            "IncludedDefaultValue": 0,
            "Minimum": "@Reward_Min",
            "Maximum": 100,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Maximum amount of item drops to reward.",
            "Variable": "rewardMax",
            "InclusiveWith":
            [
                {
                    "Key": "Reward_ID",
                    "Condition":
                    {
                        "Variable": "Reward_ID",
                        "Operation": "gt",
                        "Comparand": 0
                    }
                }
            ],
            "ExclusiveWith":
            [
                {
                    "Key": "Forage",
                    "Value": true
                }
            ]
        },
        {
            "Key": "BladeID",
            "Type": "BladeId",
            "DefaultValue": 0,
            "Description": "An arbitrary ID that weapons must also define to be able to damage this resource.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Variable": "bladeID"
        },
        {
            "Key": "Vulnerable_To_Fists",
            "Type": "Boolean",
            "DefaultValue": false,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Whether or not this resource can be damaged by punching.",
            "Variable": "vulnerableToFists"
        },
        {
            "Key": "Vulnerable_To_All_Melee_Weapons",
            "Type": "Boolean",
            "DefaultValue": false,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Whether or not this resource can be damaged by melee weapons that don't share a blade ID with this resource.",
            "Variable": "vulnerableToAllMeleeWeapons"
        },
        {
            "Key": "Ignore_Collision_Between_Stump_And_Debris",
            "Type": "Boolean",
            "DefaultValue": true,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Whether or not the @($bndl$::Stump) and @($bndl$::Debris) prefabs should be able to pass through each other.",
            "Variable": "ShouldIgnoreCollisionBetweenStumpAndDebris"
        },
        {
            "Key": "Reset",
            "Type": "Float32",
            "DefaultValue": 0.0,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Number of seconds after this resource is destroyed when it respawn.",
            "Variable": "reset"
        },
        {
            "Key": "Forage",
            "Type": "Flag",
            "DefaultValue": false,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Whether or not this resource can be interacted with to collect rewards.",
            "Variable": "isForage"
        },
        {
            "Key": "Forage_Reward_Experience",
            "Type": "UInt32",
            "DefaultValue": 1,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Amount of experience to reward when the resource is foraged from.",
            "Variable": "forageRewardExperience",
            "InclusiveWith":
            [
                {
                    "Key": "Forage",
                    "Value": true
                }
            ]
        },
        {
            "Key": "No_Debris",
            "Type": "Flag",
            "DefaultValue": false,
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Description": "Indicates that this resource does not have debris that should appear when it has been destroyed. This also affects item and explosion effect spawn positions.",
            "Variable": "hasDebris"
        },
        {
            "Key": "Holiday_Restriction",
            "Type": "SDG.Unturned.ENPCHoliday, Assembly-CSharp",
            "DefaultValue": "NONE",
            "Description": "When set, the only holiday during which this resource will be visible.",
            "Variable": "holidayRestriction",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties"
        },
        {
            "Key": "Christmas_Redirect",
            "Type":
            {
                "Type": "AssetReferenceString",
                "AssetType": "SDG.Unturned.ResourceAsset, Assembly-CSharp"
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "The resource to replace this resource with during the 'Christmas' holiday.",
            "Variable": "christmasRedirect",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties"
        },
        {
            "Key": "Halloween_Redirect",
            "Type":
            {
                "Type": "AssetReferenceString",
                "AssetType": "SDG.Unturned.ResourceAsset, Assembly-CSharp"
            },
            "DefaultValue": "00000000-0000-0000-0000-000000000000",
            "Description": "The resource to replace this resource with during the 'Halloween' holiday.",
            "Variable": "halloweenRedirect",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties"
        },
        {
            "Key": "Chart",
            "Type": "SDG.Unturned.EObjectChart, Assembly-CSharp",
            "DefaultValue": "NONE",
            "Description": "Overrides the material used for this resource when rendering the level chart.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties",
            "Variable": "chart"
        },
        {
            "Key": "Exclude_From_Level_Batching",
            "Type": "Boolean",
            "DefaultValue": false,
            "Description": "Whether or not this resource is excluded from the level batching process.",
            "Variable": "shouldExcludeFromLevelBatching",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#resource-properties"
        },
        {
            "Key": "Armor_FalloffMaxRange",
            "Type": "Float32",
            "DefaultValue": -1.0,
            "Minimum": "@Armor_FalloffRange",
            "Except": [ -1 ],
            "Description": "Ranged damage (guns) from greater than this distance finishes decreasing toward falloff multiplier. -1 indicates that falloff should be ignored.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/barricade-asset.html#barricade-asset-properties",
            "Variable": "ArmorFalloffMaxRange",
            "Version": "3.25.10.0"
        },
        {
            "Key": "Armor_FalloffRange",
            "Type": "Float32",
            "DefaultValue": "@Armor_FalloffMaxRange",
            "Minimum": 0.0,
            "Maximum": "@Armor_FalloffMaxRange",
            "Except": [ -1 ],
            "Description": "Ranged damage (guns) from greater than this distance begins decreasing toward falloff multiplier.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/barricade-asset.html#barricade-asset-properties",
            "Variable": "ArmorFalloffRange",
            "Version": "3.25.10.0"
        },
        {
            "Key": "Armor_FalloffMultiplier",
            "Type": "Float32",
            "DefaultValue": 1.0,
            "Minimum": 0.0,
            "Maximum": 1.0,
            "Except": [ -1 ],
            "Description": "Percentage of incoming damage to apply past @Armor_FalloffMaxRange.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/items/barricade-asset.html#barricade-asset-properties",
            "Variable": "ArmorFalloffMultiplier",
            "Version": "3.25.10.0"
        }
    ],
    "Localization":
    [
        {
            "Key": "Name",
            "Type":
            {
                "Type": "String",
                "SupportsRichText": true
            },
            "Required": true,
            "DefaultValue": "Name",
            "Description": "Localized display name of the resource.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#localization",
            "Variable": "resourceName"
        },
        {
            "Key": "Interact",
            "Type":
            {
                "Type": "String",
                "SupportsRichText": true,
                "FormatArguments": 1
            },
            "DefaultValue": "Interact",
            "Description": "Localized text displayed when the character looks at this resource. The keybind is formatted into '{0}'.",
            "Docs": "https://docs.smartlydressedgames.com/en/stable/assets/resource-asset.html#localization",
            "Variable": "interactabilityText",
            "InclusiveWith":
            [
                {
                    "Key": "Forage",
                    "Value": true
                }
            ]
        }
    ],
    "BundleAssets":
    [
        {
            "Key": "Resource_Clip",
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Description": "Prefab of the resource (visible when this resource is alive) only loaded on the dedicated server. Should have collision but not renderers.",
            "Deprecated": true,
            "Required":
            {
                "Variable": "Has_Clip_Prefab",
                "Operation": "eq",
                "Comparand": true
            },
            "Variable": "modelGameObject",
            "ExclusiveWith":
            [
                {
                    "Key": "Has_Clip_Prefab",
                    "Value": false
                }
            ]
        },
        {
            "Key": "Stump_Clip",
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Description": "Prefab of the stump (visible when this resource is not alive) only loaded on the dedicated server. Should have collision but not renderers.",
            "Deprecated": true,
            "Required":
            {
                "Variable": "Has_Clip_Prefab",
                "Operation": "eq",
                "Comparand": true
            },
            "Variable": "stumpGameObject",
            "ExclusiveWith":
            [
                {
                    "Key": "Has_Clip_Prefab",
                    "Value": false
                }
            ]
        },
        {
            "Key": "Resource",
            "Aliases": [ "Resource_Old" ],
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Description": "Prefab of the resource loaded on the client (and dedicated server if @Has_Clip_Prefab is false), which is visible when this resource is alive.",
            "Required": true,
            "Variable": "modelGameObject"
        },
        {
            "Key": "Stump",
            "Aliases": [ "Stump_Old" ],
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Description": "Prefab of the stump loaded on the client (and dedicated server if @Has_Clip_Prefab is false), which is visible when this resource is not alive.",
            "Required": true,
            "Variable": "stumpGameObject"
        },
        {
            "Key": "Skybox",
            "Aliases": [ "Skybox_Old" ],
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Description": "Prefab of the model loaded on clients when this resource is very far away and alive.",
            "Variable": "skyboxGameObject"
        },
        {
            "Key": "Debris",
            "Aliases": [ "Debris_Old" ],
            "Type": "UnityEngine.GameObject, UnityEngine.CoreModule",
            "Description": "Prefab of the resource that falls on the client when this resource is destroyed.",
            "Variable": "debrisGameObject",
            "ExclusiveWith":
            [
                {
                    "Key": "No_Debris",
                    "Value": true
                }
            ]
        }
    ]
}